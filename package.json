{
  "name": "servie-cors",
  "version": "0.0.1",
  "description": "CORS middleware for Servie",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "build": "rm -rf dist/ && tsc",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/serviejs/servie-cors.git"
  },
  "keywords": [
    "servie",
    "cors",
    "origin",
    "resource",
    "sharing",
    "xhr",
    "http"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/serviejs/servie-cors/issues"
  },
  "homepage": "https://github.com/serviejs/servie-cors",
  "devDependencies": {
    "jest": "^19.0.0",
    "rimraf": "^2.5.4",
    "throwback": "^2.0.0",
    "ts-jest": "^19.0.0",
    "tslint": "^5.0.0",
    "tslint-config-standard": "^5.0.0",
    "typescript": "^2.2.1",
    "typings": "^2.1.0"
  },
  "dependencies": {
    "servie": "^0.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*\\.(?:test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
